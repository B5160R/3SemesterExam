@page
@model Webapp.Pages.Booking.UpdateModel
@{
}

<div class="row">
    <div class="col-md-6">
        <form method="post">
            <div class="form-group">
                <label asp-for="UpdateViewModel.Title"></label>
                <input asp-for="UpdateViewModel.Title" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="UpdateViewModel.StartDato"></label>
                <input asp-for="UpdateViewModel.StartDato" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="UpdateViewModel.SlutDato"></label>
                <input asp-for="UpdateViewModel.SlutDato" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="UpdateViewModel.Kommentar"></label>
                <input asp-for="UpdateViewModel.Kommentar" class="form-control" />
            </div>
            <button class="btn btn-success" type="submit" class="btn btn-default">Rediger</button>
        </form>
    </div>
</div>