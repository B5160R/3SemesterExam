@page
@model Webapp.Pages.Medarbejder.CreateModel
@{
}
<h2>Opret UNIK Medarbejder</h2>
<div class="row">
    <div class="col-md-6">
        <form method="post">
            <div class="form-group">

                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <label asp-for="CreateViewModel.Name"></label>
                <input asp-for="CreateViewModel.Name" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="CreateViewModel.ProcessRole"></label>
                <input asp-for="CreateViewModel.ProcessRole" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="CreateViewModel.Email"></label>
                <input asp-for="CreateViewModel.Email" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="CreateViewModel.PhoneNr"></label>
                <input asp-for="CreateViewModel.PhoneNr" class="form-control" />
            </div>
            <button class="btn btn-success" type="submit" class="btn btn-default">Opret</button>
        </form>
    </div>
</div>